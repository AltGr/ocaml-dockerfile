description = "OCaml interface for generating Dockerfiles"
requires = "sexplib"
archive(byte) = "dockerfile.cma"
archive(native) = "dockerfile.cmxa"
plugin(native) = "dockerfile.cmxs"
plugin(byte) = "dockerfile.cma"
package "opam-cmdliner" (
 version = "%%VERSION%%"
 description = "Cmdliner terms for Dockerfile CLIs"
 requires = "dockerfile dockerfile.opam cmdliner"
 archive(byte) = "dockerfile_opam_cmdliner.cma"
 archive(native) = "dockerfile_opam_cmdliner.cmxa"
 plugin(byte) = "dockerfile_opam_cmdliner.cma"
 plugin(native) = "dockerfile_opam_cmdliner.cmxs"
 exists_if = "dockerfile_opam_cmdliner.cma"
)
package "opam" (
 version = "%%VERSION%%"
 description = "OPAM and distribution support for Dockerfiles"
 requires = "dockerfile sexplib str bytes"
 archive(byte) = "dockerfile_opam.cma"
 archive(native) = "dockerfile_opam.cmxa"
 plugin(byte) = "dockerfile_opam.cma"
 plugin(native) = "dockerfile_opam.cmxs"
 exists_if = "dockerfile_opam.cma"
)
