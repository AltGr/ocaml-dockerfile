version = "%%VERSION%%"
description = "OCaml interface for generating Dockerfiles"
requires = "sexplib"
archive(byte) = "dockerfile.cma"
archive(byte, plugin) = "dockerfile.cma"
archive(native) = "dockerfile.cmxa"
archive(native, plugin) = "dockerfile.cmxs"
package "opam-cmdliner" (
 version = "%%VERSION%%"
 description = "Cmdliner terms for Dockerfile CLIs"
 requires = "dockerfile dockerfile.opam cmdliner"
 archive(byte) = "opam-cmdliner.cma"
 archive(byte, plugin) = "opam-cmdliner.cma"
 archive(native) = "opam-cmdliner.cmxa"
 archive(native, plugin) = "opam-cmdliner.cmxs"
 exists_if = "opam-cmdliner.cma"
)
package "opam" (
 version = "%%VERSION%%"
 description = "OPAM and distribution support for Dockerfiles"
 requires = "dockerfile sexplib str bytes"
 archive(byte) = "opam.cma"
 archive(byte, plugin) = "opam.cma"
 archive(native) = "opam.cmxa"
 archive(native, plugin) = "opam.cmxs"
 exists_if = "opam.cma"
)
